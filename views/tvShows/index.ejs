<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" type="text/css" href="css/style.css" />
    <title>TV Shows</title>
</head>

<body>
    <h1>TV Shows</h1>
    <ul>
        <% tvShows.forEach(function(show) { %>
        <li>
            <%= show.title %>
            -
            <%= show.stream %>
        </li>
        <% }); %>
    </ul>
</body>

</html>