<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" type="text/css" href="css/style.css" />
    <title>Games</title>
</head>

<body>
    <h1>Games</h1>
    <ul>
        <% games.forEach(function(game) { %>
        <li>
            <%= game.title %>
            -
            <%= game.released ? 'Released' : 'Not released' %>
        </li>
        <% }); %>
    </ul>
</body>

</html>